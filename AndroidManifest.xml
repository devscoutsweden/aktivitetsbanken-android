<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="se.devscout.android"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-sdk android:minSdkVersion="15" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:label="@string/app_name"
        android:icon="@drawable/launcher"
        android:logo="@drawable/action_bar"
        android:theme="@style/DevScout">
        <activity
            android:name=".controller.activity.MainActivity"
            android:label="@string/app_name"></activity>
        <activity android:name=".controller.activity.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".controller.activity.ActivityActivity"></activity>
        <activity android:name=".controller.activity.ActivitiesActivity"></activity>
        <activity android:name=".controller.activity.SearchResultActivity"></activity>
        <activity android:name=".controller.activity.FragmentCreatorActivity"></activity>
        <activity android:name=".controller.activity.DatabaseActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
    </application>
</manifest>
